        <LogLevel>DEBUG</LogLevel>
        <LoggerName>FpOnlineRateTable</LoggerName>
        <MaxFiles>10</MaxFiles>
    </GlobalLogger>
    <FpOnlineRateCalculatorWidget>
        <Id>fp_online_rate_calculator</Id>
        <HtmlClass>fp-online-rate-calculator</HtmlClass>
        <Shortcode>fp_online_rate_calculator</Shortcode>
        <DefaultSettings>
            <GetActiveRateTablesPath>RateCalculation/GetActiveTables</GetActiveRateTablesPath>
            <RateCalculationStartPath>RateCalculation/Start</RateCalculationStartPath>
