        </DefaultSettings>
    </FpOnlineRateCalculatorWidget>
    <CustomJavascriptWrapper>
        <IdPrefix>fp_online_rate_calculator-</IdPrefix>
        <LocalizationObject>localization</LocalizationObject>
        <MetaObject>meta</MetaObject>
    </CustomJavascriptWrapper>
</FpOnlineRateTable>