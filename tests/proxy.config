        <!-- testcase: http-when-https -->
        <serverUrl url="https://sampleserver6.arcgisonline.com/arcgis/rest/services/Utilities" matchAll="true"/>

        <!-- testcase: federation-only, issue #191 -->
        <serverUrl url="http://route.arcgis.com"
                   matchAll="true"
                   clientId="gYTkNoqB5PUK4w18"
